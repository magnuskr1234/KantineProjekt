<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
    <servlet-name>PersonServiceServlet</servlet-name>
    <servlet-class>gwt.server.ConnectionDB</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>PersonServiceServlet</servlet-name>
    <url-pattern>/kantine_v2/personservice</url-pattern>
  </servlet-mapping>
  
    <servlet>
    <servlet-name>ItemServiceServlet</servlet-name>
    <servlet-class>gwt.server.ConnectionDB</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>ItemServiceServlet</servlet-name>
    <url-pattern>/kantine_v2/itemservice</url-pattern>
  </servlet-mapping>


  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Kantine_v2.html</welcome-file>
  </welcome-file-list>

</web-app>
