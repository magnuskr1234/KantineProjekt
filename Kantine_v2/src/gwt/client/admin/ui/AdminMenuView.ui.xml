<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel styleName="block-bg">
		<g:Grid width="100%" height="100%" styleName="center">
			<g:row>
				<g:customCell>
					<g:Button ui:field="createUser">Opret bruger</g:Button>
				</g:customCell>
				<g:customCell>
					<g:Button ui:field="createItem">Opret vare</g:Button>
				</g:customCell>
				<g:customCell>
					<g:Button ui:field="deleteItem" text="Slet vare"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Button ui:field="statistic">Statistik</g:Button>
				</g:customCell>
				<g:customCell>
					<g:Button ui:field="deleteUser">Slet bruger</g:Button>
				</g:customCell>
				<g:customCell>
					<g:Button ui:field="showUsers">Vis brugere</g:Button>
				</g:customCell>
			</g:row>
			<g:row/>
		</g:Grid>
	</g:HTMLPanel>
</ui:UiBinder> 