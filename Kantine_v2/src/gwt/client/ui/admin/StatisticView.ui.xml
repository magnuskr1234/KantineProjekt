<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel height="100%">
		<g:VerticalPanel width="100%" height="30%" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_TOP">
			<g:HorizontalPanel width="80%" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE" styleName="whiteBackground" height="85%">
				<g:Label text="Total earnings" ui:field="totearn" styleName="blackText" width="100%"/>
				<g:VerticalPanel width="" horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE" height="">
					<g:FlexTable ui:field="mostSoldTable" width=""/>
				</g:VerticalPanel>
			</g:HorizontalPanel>
			<g:HorizontalPanel horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE" width="80%" height="100%" styleName="whiteBackground" spacing="10">
				<g:Label text="Bruger" styleName="blackText_Bold" horizontalAlignment="ALIGN_LEFT" width="100px"/>
				<g:Label text="Vare" styleName="blackText_Bold" horizontalAlignment="ALIGN_LEFT" width="100px"/>
				<g:Label text="Pris" styleName="blackText_Bold" horizontalAlignment="ALIGN_LEFT" width="100px"/>
				<g:Label text="Dato" styleName="blackText_Bold" horizontalAlignment="ALIGN_LEFT" width="100px"/>
			</g:HorizontalPanel>
		</g:VerticalPanel>
		<g:HorizontalPanel width="100%" height="70%" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_TOP">
		<g:ScrollPanel height="100%" width="100%">
			<g:FlexTable ui:field="statTable" width="80%" height="80%" styleName="whiteBackground" cellSpacing="10"/>
		</g:ScrollPanel>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 