<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel width="100%" height="100%"
		horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
		<g:VerticalPanel height="350px" width="300px" horizontalAlignment="ALIGN_CENTER" styleName="loginContainer" verticalAlignment="ALIGN_MIDDLE">
			<g:Image url="ic_account_circle_white_48dp_2x.png" width="50px"/>
			<g:TextBox ui:field="txtName" width="200px" height="25px"/>
			<g:TextBox ui:field="txtPassword" width="200px" height="25px"/>
			<g:CheckBox text="Administrator" ui:field="radioAdmin" styleName="whiteText" width="150px"/>
			<g:TextBox ui:field="txtSaldo" width="200px" height="25px"/>
			<g:HorizontalPanel>
				<g:Button ui:field="createUserBtn" styleName="loginBtn" width="200px" height="35px">Opret</g:Button>
			</g:HorizontalPanel>
		</g:VerticalPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 