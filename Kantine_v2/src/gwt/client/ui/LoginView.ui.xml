<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:w="urn:import:com.google.gwt.widget.client">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel width="300px" height="300px">
		<g:VerticalPanel spacing="2" styleName="loginContainer" width="300px" height="300px" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
			<g:Label text="Brugernavn" width="210px" horizontalAlignment="ALIGN_LEFT" styleName="whiteText"/>
			<g:TextBox alignment="LEFT" ui:field="txtUid" height="25px" width="200px"/>
			<g:Label text="Adgangskode" width="210px" horizontalAlignment="ALIGN_LEFT" styleName="whiteText"/>
			<g:PasswordTextBox alignment="LEFT" ui:field="txtPw" height="25px" width="200px"/>
			<g:Cell height="">
				<g:HorizontalPanel spacing="5" width="100px" height="" horizontalAlignment="ALIGN_CENTER">
				<g:Cell height="">
					<g:Button text="Log ind" ui:field="btnOk" height="35px" width="210px" styleName="loginBtn"/>
				</g:Cell>
			</g:HorizontalPanel>
			</g:Cell>
			<g:Label text="Brugernavn eller adgangskode er indtastet forkert" width="193px" ui:field="lblLoginError" styleName="whiteText"/>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 