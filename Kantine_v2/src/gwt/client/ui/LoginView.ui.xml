<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:w="urn:import:com.google.gwt.widget.client">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel spacing="2" styleName="login">
			<g:Label text="Brugernavn:"/>
			<g:TextBox alignment="LEFT" ui:field="txtUid" height="8px" width="100px"/>
			<g:Label text="Kodeord:"/>
			<g:PasswordTextBox alignment="LEFT" ui:field="txtPw" height="8px" width="100px"/>
			<g:Cell height="">
				<g:HorizontalPanel spacing="5" width="100px" height="" horizontalAlignment="ALIGN_CENTER">
				<g:Cell height="">
					<g:Button text="Ok" ui:field="btnOk" height="" width=""/>
				</g:Cell>
				<g:Button text="Fortryd" ui:field="btnCancel"/>
			</g:HorizontalPanel>
			</g:Cell>
			<g:Label text="Brugernavn eller kodeord forkert" width="100px" ui:field="lblLoginError" styleName="login-error"/>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 