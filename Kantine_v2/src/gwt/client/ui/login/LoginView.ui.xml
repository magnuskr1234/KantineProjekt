<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:w="urn:import:com.google.gwt.widget.client">
	<ui:style>
	
	</ui:style>
	<g:VerticalPanel width="100%" height="100%"
		horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
		<g:VerticalPanel spacing="2" styleName="loginContainer"
			width="300px" height="300px" horizontalAlignment="ALIGN_CENTER"
			verticalAlignment="ALIGN_MIDDLE">
			<g:TextBox alignment="LEFT" ui:field="txtUid" height="25px" 
				width="200px" />
			<g:PasswordTextBox alignment="LEFT" ui:field="txtPw"
				height="25px" width="200px" />
			<g:Cell height="">
				<g:HorizontalPanel spacing="5" width="100px"
					height="" horizontalAlignment="ALIGN_CENTER">
					<g:Cell height="">
						<g:Button text="Log ind" ui:field="btnOk" height="35px"
							width="210px" styleName="loginBtn" />
					</g:Cell>
				</g:HorizontalPanel>
			</g:Cell>
			<g:Label text="Brugernavn eller adgangskode er indtastet forkert"
				width="225px" ui:field="lblLoginError" styleName="whiteText" />
		</g:VerticalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 